type Query {
    hello: String
    book(isbn: ID!): Book
    timeline(id: ID!): Timeline
}


type Author {
	name: String!
}

type Book {
	isbn: ID!
	title: String!
	author: Author!
}


type Timeline {
	id: ID!
	tweets: [Tweet]
}

type Tweet {
	text: String!
	user: User!
}

type User {
	name: String!
}
